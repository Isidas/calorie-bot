# Telegram Bot Token from @BotFather
TELEGRAM_BOT_TOKEN=YOUR_TELEGRAM_BOT_TOKEN

# Google Gemini API Key (vision + fallback estimate)
GEMINI_API_KEY=YOUR_GEMINI_API_KEY

# Gemini model (default: gemini-2.5-flash, free tier; if 404 see ai.google.dev/api/models)
# GEMINI_MODEL=gemini-2.5-flash

# USDA FoodData Central API Key (data.gov / FDC signup)
USDA_API_KEY=YOUR_USDA_API_KEY

# HTTP timeout for Gemini and USDA requests in ms (default 30000)
# HTTP_TIMEOUT_MS=30000

# If true, when no USDA match is found the bot will ask Gemini to estimate nutrition (default false)
ENABLE_GEMINI_FALLBACK=false
